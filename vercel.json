{
  "redirects": [
    {
      "source": "/portfolio",
      "destination": "/projects/",
      "permanent": true
    },
    {
      "source": "/projects/:subpath(.+)",
      "destination": "https://projects.calebevans.me/:subpath",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=15552000; includeSubDomains"
        }
      ]
    }
  ]
}
