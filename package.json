    "schema-dts": "^1.1.2",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tslib": "^2.6.2",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-imagetools": "^5.0.8"
