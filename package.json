    "puppeteer": "^21.1.1",
    "sass": "^1.63.6",
    "schema-dts": "^1.1.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.4",
    "tslib": "^2.5.3",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-imagetools": "^5.0.4"
  },
  "type": "module"
}
