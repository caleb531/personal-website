{
  "name": "personal-website",
  "version": "3.0.0",
  "private": true,
  "description": "The personal website of Caleb Evans, coder for Christ.",
  "author": "Caleb Evans",
  "keywords": [
    "svelte",
    "sveltekit"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
